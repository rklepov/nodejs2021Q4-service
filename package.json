{
  "name": "rs-rest-service-starter",
  "version": "1.2.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build:docs": "typedoc",
    "clean:logs": "del-cli ./logs/*.log",
    "db:migration:gen": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate -p -n",
    "db:migration:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "db:migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "dev": "nodemon",
    "dev:docs": "nodemon --exec npm run build:docs",
    "eslint": "eslint --ext .js,.ts --fix src/",
    "lint": "npm run build:check && npm run eslint",
    "prepare": "husky install",
    "start": "npm run db:migration:run && npm run start:tsnode",
    "start:container": "npm run db:migration:run && nodemon -L --exitcrash --config nodemon-docker.json",
    "start:tsnode": "ts-node src/server.ts",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace"
  },
  "keywords": [
    "autotest",
    "fastify",
    "node",
    "rest",
    "rs",
    "school",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix-dry-run"
    ]
  },
  "dependencies": {
    "cross-env": "7.0.3",
    "dayjs": "^1.10.7",
    "dotenv": "^12.0.3",
    "fastify": "^3.25.3",
    "fastify-sensible": "^3.1.2",
    "fastify-swagger": "^4.13.1",
    "http-status": "^1.5.0",
    "lodash.difference": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "pg": "^8.7.1",
    "pino": "^7.6.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/lodash.difference": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/node": "^17.0.8",
    "@types/pino-pretty": "^4.7.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "4.3.4",
    "debug": "4.3.3",
    "del-cli": "^4.0.1",
    "dirty-chai": "2.0.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.4",
    "jest": "27.4.7",
    "lint-staged": "^12.1.7",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.3.0",
    "prettier": "^2.5.1",
    "supertest": "6.2.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "optionalDependencies": {
    "npm-check-updates": "^12.1.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-mdn-links": "^1.0.4",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typedoc-plugin-rename-defaults": "^0.4.0"
  }
}
