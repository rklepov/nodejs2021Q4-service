{
  "name": "nodejs2021-q4-service@nest",
  "version": "3.0.0",
  "description": "REST service starter for RS School",
  "author": "Roman Klepov",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rklepov/nodejs2021Q4-service"
  },
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "clean:logs": "del-cli ./logs/*.log",
    "db:migration:gen": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate -p -n",
    "db:migration:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "db:migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace"
  },
  "keywords": [
    "autotest",
    "nest",
    "node",
    "rest",
    "rs",
    "school",
    "starter"
  ],
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix-dry-run"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/platform-fastify": "^8.2.6",
    "@nestjs/typeorm": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "cross-env": "7.0.3",
    "dayjs": "^1.10.7",
    "dotenv": "^14.3.2",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chai": "4.3.6",
    "debug": "4.3.3",
    "del-cli": "^4.0.1",
    "dirty-chai": "2.0.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "27.4.7",
    "lint-staged": "^12.3.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "6.2.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "optionalDependencies": {
    "npm-check-updates": "^12.2.1"
  }
}
